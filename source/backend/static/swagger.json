    ,
    

        "paths": {
            "/process/deploy":{
                "post":{
                    "summary" : "deploys certain processes to engine",
                    "responses": {
                        "200":{
                            "description": "OK"
                        }
                        
                    }
                }
            }
        

        

        ,

        "/process/{process_id}/start_instance":{
            "post":{
                "summary" : "start a process instance in the engine",
                "parameters":[{
                  "in":"path",
                  "name":"process_id",
                  "required":"true",
                  "description":"id of process definition to start instance of",
                  "schema" :{
                      "type":"string"
                  }
                }],
                "responses": {
                    "200":{
                        "description": "OK"
                    }
                    ,"400":{
                        "description":"could not create instance"
                    }
                }
            }
        }

        ,


        "/process/{process_id}/start_instances":{
            "post":{
                "summary" : "start a process instance a certain number of times in the engine",
                "parameters":[{
                    "in":"path",
                    "name":"process_id",
                    "required":"true",
                    "description":"id of process definition to start instance of",
                    "schema" :{
                        "type":"string"
                    }
                  }
                

           
                
                
                ],


                "requestBody":{
                    "content": {
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "count":{
                                        "type":"integer"
                                    }
                                }
                            }
                        }
                    }
                }
                ,
                








                "responses": {
                    "200":{
                        "description": "OK"
                    }
                }
                ,"400":{
                    "description":"could not create some/all instances"
                }
            }
        }
,
"/process/definition":{
    "delete":{
        "summary" : "deletes all process definitions",
        "responses": {
            "200":{
                "description": "OK"
            }
        }
    }
}

,
"/process/instance":{
    "delete":{
        "summary" : "deletes all process instance",
        "responses": {
            "200":{
                "description": "OK"
            }
        }
    }
}




,
"/process/all":{
    "delete":{
        "summary" : "deletes all process data from engine",
        "responses": {
            "200":{
                "description": "OK"
            }
        }
    }
}

,
"/process/history":{
    "get":{
        "summary" : "polls engine history service",
        "responses": {
            "200":{
                "description": "OK"
            }
        }
    }
}




    }
}
  